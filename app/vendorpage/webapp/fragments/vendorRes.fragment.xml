<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Dialog id="idresevednd" title="Reservation of ParkingLot">
        <beginButton>
            <Button
                id="ksks"
                text="Confirm"
                type="Success"
                press="onConfirmDialog"
            />
        </beginButton>
        <endButton>
            <Button
                id="kdkja"
                text="Cancel"
                type="Negative"
                press="onCloseDialog"
            />
        </endButton>
        <content>
            <form:SimpleForm
                id="idsseims"
                editable="false"
            >
                <Label
                    id="idttielvallabel"
                    text="Truck Number:"
                />
                <Input
                    id="idventruckno"
                    value="{}"
                />
                <Label
                    id="idvendorDrivername"
                    text="Driver Name"
                />
                <Input
                    id="idvendrivername"
                    value="{}"
                />
                <Label
                    id="idlabel"
                    text="Driver Mobile"
                />
                <Input
                    id="idvenddrivermob"
                    value="{  }"
                />
                <Label
                    id="idlabelVendorName"
                    text="Vendor Name:"
                />
                <Input
                    id="idvednorval"
                    value="{}"
                />
            </form:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>
