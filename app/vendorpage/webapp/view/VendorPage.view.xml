<mvc:View
    controllerName="com.app.vendorpage.controller.VendorPage"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <headerContent>
            <ToolbarSpacer id="idttool" />
            <Button
                id="iddbbuu"
                text="Reservation"
                press="onReservePress"
            />
        </headerContent>
        <content>
            <Image
                id="idvendorimage"
                width="100%"
                src="https://i.postimg.cc/sD7KXDSR/transportation-and-logistic-import-export-and-transport-industry-of-truck-container-cargo-ship-photo.jpg"
            />
        </content>
    </Page>
</mvc:View>
